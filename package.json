{
  "name": "tsuibami",
  "version": "0.1.0",
  "description": "mini editor for esa.io",
  "main": "index.js",
  "scripts": {
    "build:prepare": "mkdir -p build/",
    "build:js": "cp -a src/js/ build/",
    "build:lib": "cp -a lib/ build/lib/",
    "build": "npm run build:prepare && npm run build:js && npm run build:lib",
    "_build": "gulp build",
    "rebuild": "gulp rebuild",
    "release": "gulp release",
    "watch": "gulp default"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:yoshihara/tsuibami.git"
  },
  "keywords": ["esa.io"],
  "author": "yoshihara",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yoshihara/tsuibami/issues"
  },
  "homepage": "https://github.com/yoshihara/tsuibami",
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "del": "^2.0.2",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.5.2",
    "gulp-download": "0.0.1",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-ruby-haml": "0.0.4",
    "gulp-sass": "^2.0.4",
    "gulp-uglify": "^1.1.0",
    "gulp-zip": "^3.0.2",
    "run-sequence": "^1.1.4"
  }
